<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>email</th>
    <th>password</th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.firstname %></td>
      <td><%= user.lastname %></td>
      <td><%= user.email %></td>
      <td><%= user.password %></td>

      <td><%= link_to "EDIT", user_edit_path(user)%></td>
       <td><%= link_to "DELETE", user_destroy_path(user), method: :delete, data:{
        confirm: "Are you sure?"
        } %></td>
    </tr>
  <% end %>
</table>